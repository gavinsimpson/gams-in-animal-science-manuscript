  language = {en}
}
@ARTICLE{Macciotta2011-ch,
  title = {The mathematical description of lactation curves in dairy cattle},
  author = {Macciotta, Nicolò P P and Dimauro, Corrado and Rassu, Salvatore P G
  and Steri, Roberto and Pulina, Giuseppe},
  journal = {Ital. J. Anim. Sci.},
